pip completion [-b | --bash] [-z | --zsh];
pip download [-r <txt> | --requirement <txt>];
pip freeze;
pip hash [-a <algo> | --algorithm <algo>];
pip help;
pip install [-r <txt> | --requirement <txt>];
pip list;
pip search;
pip show;
pip uninstall [-r <txt> | --requirement <txt>] [-y | --yes];
pip wheel;

algo = sha256 sha384 sha512 ;
txt = ! ls *.txt 2>/dev/null ;
